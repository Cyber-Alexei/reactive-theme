	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#414655"
			}
		},
		{
			"name": "NEW",
			"scope": [
				"meta.definition.function"
			],
			"settings": {
				"foreground": "#77c5b8"
			}
		},
		{
			"name": "Variable Constant",
			"scope": [
				"variable.other.constant",
			],
			"settings": {
				"foreground": "#F6F6F7"
			}
		},
		{
			"name": "Variable Object",
